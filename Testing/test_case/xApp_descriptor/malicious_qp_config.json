{
    "xapp_name": "fake_qp",
    "version": "0.0.1",
    "containers": [
        {
            "name": "fake_qp",
            "image": {
                "registry": "self_defined_registry:10002",
                "name": "fake_qp",
                "tag": "0.0.1"
            }
        }
    ],
    "messaging": {
        "ports": [
            {
                "name": "rmr-data",
                "container": "fake_qp",
                "port": 4560,
                "rxMessages": ["TS_UE_LIST"],
                "txMessages": ["TS_QOE_PREDICTION"],
                "policies": [],
                "description": "rmr receive data port for qp"
            },
            {
                "name": "rmr-route",
                "container": "fake_qp",
                "port": 4561,
                "description": "rmr route port for qp"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "rxMessages": ["TS_UE_LIST"],
        "txMessages": ["TS_QOE_PREDICTION"],
        "policies": []
    }
}
